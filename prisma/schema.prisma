// prisma/schema.prisma
datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

model Pegawai{
  id String @id @default(cuid())
  name String
  email String
  phone String
  shift String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}